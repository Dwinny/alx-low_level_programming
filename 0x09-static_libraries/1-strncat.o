#include "main.h"
/**
 * _strncat - it concatenates two strings
 * use at most n byte from src
 * @dest: it inputs value
 * @src:it inputs value
 * @n:it inputs value
 *
 * Return: dest
 */
char *_strncat(char *dest, char *src, int n)
{
	int y;
	int x;

	y = 0;
	while (dest[y] != '\0')
	{
		y++;
	}
	x = 0;
	while (x < n && src[x] != '\0')
	{
	dest[x] = src[x];
	y++;
	x++;
	}
	dest[y] = '\0';
	return (dest);
}
